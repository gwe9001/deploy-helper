{
  "name": "deploy-helper",
  "productName": "deploy-helper",
  "version": "1.0.13",
  "description": "Deploy Helper",
  "repository": "github:gwe9001/deploy-helper",
  "main": ".vite/build/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:nuxt\" \"npm run dev:electron\"",
    "dev:nuxt": "nuxt dev",
    "dev:electron": "electron-forge start",
    "build": "nuxt generate && electron-forge package",
    "generate": "nuxt generate",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "nuxt generate && electron-forge make",
    "publish": "nuxt generate && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-dmg": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron-forge/plugin-vite": "^7.5.0",
    "@electron-forge/publisher-github": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@eslint/js": "^9.12.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.0",
    "electron": "32.1.2",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.0",
    "element-plus": "^2.8.4",
    "eslint": "^9.12.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.10.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.6",
    "is-electron": "^2.2.2",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.0.16",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.0.12",
    "vitest": "^3.2.4",
    "vue": "^3.5.11",
    "vue-router": "4"
  },
  "keywords": [],
  "author": {
    "name": "Jim Wang"
  },
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@element-plus/nuxt": "^1.1.4",
    "electron-log": "^5.2.0",
    "nuxt": "^3.17.7",
    "update-electron-app": "^3.0.0",
    "vue-draggable-plus": "^0.5.3"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  }
}
